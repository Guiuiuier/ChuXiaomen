<script>
    var allow = "2";
    var identify = "1";
    window.onload = function () {
        if (allow == 1) {
            $("#bgm").removeClass('main');
            $("#bgm").addClass('main-allow');
            $("#bgm").css('background-color', '#FF9900');
            if (identify > 1) {
                if (identify == 6){
                    $(".main-allow").css('background-color', '#FF9900');
                } else{
                    $(".main-allow").css('background-color', 'rgb(25,122,255)');
                }
            }
            $("#img-allow").css('height', '232px');
            $("#img-allow").css('width', '100%');
            $(".content1").css('padding-top', '1vh');
        }
    }

    setInterval(getCurrentTime, 1000);

    function getCurrentTime() {
        let now = new Date();
        let hh = now.getHours();
        let mm = now.getMinutes() >= 10 ? now.getMinutes() : '0' + now.getMinutes();
        let ss = now.getSeconds() >= 10 ? now.getSeconds() : '0' + now.getSeconds();
        let timeStr = hh + ':' + mm + ':' + ss;
        $("#hourTime").text(timeStr);
    }
</script>

